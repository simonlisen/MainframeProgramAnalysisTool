@{
    ViewBag.Title = "Home Page";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">  
</head>
<body>
    <!--Main canvas-->
    <div id="chart-container">
        <div class="jumbotron">
            <div class="container">
                <h1>Welcome to MPAT </h1>
                <h2>Mainframe Program Analysis Tool</h2>
                <p>This is a simple tool, which is a simple jumbotron-style component for calling extra attention to featured content or information. It could help you xxx whatever...</p>
                <p><a class="btn btn-primary btn-lg" href="#" role="button" data-toggle="modal" data-target="#myModal">Let's go</a></p>
            </div>
        </div>
    </div>   
    <!--  Modal -->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;
                    </button>
                    <h4 class="modal-title" id="myModalLabel">
                        Build new flow
                    </h4>
                </div>
                <div class="modal-body">
                    <input id="ipt_buildnew" class="form-control" type="text" required="required"/>
                </div>
                <div class="modal-footer">                    
                    <button type="button" id="submit-build-new" class="btn btn-primary">
                        <i class="fa fa-check-circle"></i>Build
                    </button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">
                        Cancel
                    </button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal -->
    </div>
</body>
</html>

